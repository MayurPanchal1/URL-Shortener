{
    "manifest_version": 2,
    "name": "URL Shortener",
    "version": "0.0.1",
    "description": "Shorten URLs and copy to clipboard",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "storage"]
  }
  